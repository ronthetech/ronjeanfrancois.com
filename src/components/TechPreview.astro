---
import javascript from "../assets/icons/JavaScript.svg"
import mongodb from "../assets/icons/MongoDB.svg"
import nextjs from "../assets/icons/NextJS-Dark.svg"
import nodejs from "../assets/icons/NodeJS-Dark.svg"
import postgresql from "../assets/icons/PostgreSQL-Dark.svg"
import python from "../assets/icons/Python-Dark.svg"
import reactjs from "../assets/icons/React-Dark.svg"
import typescript from "../assets/icons/TypeScript.svg"
---

<ul class="grid grid-cols-1 gap-5 sm:grid-cols-2">
  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={javascript} alt="" height={25} width={25} />
    </span>
    <p class="text-zinc-200">JavaScript</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={typescript} alt="" height={25} width={25} />
    </span>
    <p class="text-zinc-200">TypeScript</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={nodejs} alt="" height={30} width={30} />
    </span>
    <p class="text-zinc-200">Node.js</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={reactjs} alt="" height={30} width={30} />
    </span>
    <p class="text-zinc-200">React</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={nextjs} alt="" height={30} width={30} />
    </span>
    <p class="text-zinc-200">Next.js</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={python} alt="" height={30} width={30} />
    </span>
    <p class="text-zinc-200">Python</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={postgresql} alt="" height={35} width={35} />
    </span>
    <p class="text-zinc-200">PostgreSQL</p>
  </li>

  <li class="flex items-center gap-4">
    <span
      class="w-11 h-11 flex items-center justify-center border-t border-zinc-700 bg-zinc-800 rounded-full"
      ><img src={mongodb} alt="" height={30} width={30} />
    </span>
    <p class="text-zinc-200">MongoDB</p>
  </li>
</ul>
